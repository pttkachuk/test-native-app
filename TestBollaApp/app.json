{
  "expo": {
    "name": "TestBollaApp",
    "slug": "TestBollaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/techne-logo-small.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "Allow  $(PRODUCT_NAME) to open the camera",
          "microphonePermission": false
        }
      ],
      "expo-font"
    ],
    "splash": {
      "image": "./assets/techne-splash-narrow.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/techne-logo-small.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/techne-logo-small.png"
    }
  }
}
